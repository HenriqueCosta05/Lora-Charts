{
  "name": "lora-charts",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:lib": "ng build lora-charts --configuration production",
    "watch": "ng build --watch --configuration development",
    "storybook": "ng run angular-latest:storybook",
    "build-storybook": "ng run angular-latest:build-storybook",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepublish:lib": "npm run build:lib",
    "publish:lib": "npm run build:lib && cd dist/lora-charts && npm publish --access public"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "@types/d3": "^7.4.3",
    "@types/d3-cloud": "^1.2.9",
    "d3": "^7.9.0",
    "d3-cloud": "^1.2.8",
    "rxjs": "~7.8.0",
    "topojson-client": "^3.1.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.2101.0",
    "@angular-devkit/build-angular": "^21.1.0",
    "@angular-devkit/core": "^21.1.0",
    "@angular/build": "^21.1.0",
    "@angular/cli": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/platform-browser-dynamic": "^21.1.0",
    "@compodoc/compodoc": "^1.2.1",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/angular": "^10.1.11",
    "@types/jest": "^30.0.0",
    "@types/topojson-client": "^3.1.5",
    "@types/topojson-specification": "^1.0.5",
    "@vitest/coverage-v8": "^4.0.17",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.1.0",
    "storybook": "^10.1.11",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8",
    "zone.js": "^0.16.0"
  }
}
